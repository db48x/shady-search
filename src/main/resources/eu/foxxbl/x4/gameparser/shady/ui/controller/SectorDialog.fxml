<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import io.github.palexdev.materialfx.controls.MFXTableColumn?>
<?import io.github.palexdev.materialfx.controls.MFXTableView?>
<?import io.github.palexdev.materialfx.controls.MFXTextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox fx:id="dialog" alignment="CENTER" prefHeight="500.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="eu.foxxbl.x4.gameparser.shady.ui.controller.SectorDialog">
  <MFXTextField fx:id="sectorName" style="-fx-background-color: transparent;" alignment="CENTER" editable="false" allowEdit="false" caretVisible="false" floatMode="DISABLED" selectable="false"  prefWidth="1000.0">
    <font>
      <Font name="System Bold" size="16.0"/>
    </font>
  </MFXTextField>
  <MFXTableView footerVisible="false" fx:id="blackMarketeerTable" prefHeight="400.0" prefWidth="1200.0" VBox.vgrow="ALWAYS">
    <tableColumns>
      <MFXTableColumn fx:id="stationCode" prefWidth="200.0" text="StationCode"/>
      <MFXTableColumn fx:id="stationMacro" prefWidth="400.0" text="StationMacro"/>
      <MFXTableColumn fx:id="blackMarketeerName" prefWidth="400.0" text="Black Marketeer Name"/>
      <MFXTableColumn fx:id="status" prefWidth="50.0" text="Status"/>
      <MFXTableColumn fx:id="voiceLeaks" prefWidth="200.0" text="VoiceLeaks"/>
    </tableColumns>
  </MFXTableView>
  <MFXButton fx:id="closeButton" text="Close" styleClass="outline-button" buttonType="RAISED" />

</VBox>
